{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "products": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "availabilityStatus": { "type": "string" },
          "brand": { "type": "string" },
          "category": { "type": "string" },
          "description": { "type": "string" },
          "id": { "type": "integer" },
          "price": { "type": "number" },
          "stock": { "type": "integer" },
          "title": { "type": "string" }
        },
        "required": [
          "id",
          "price",
          "title"
        ]
      }
    }
  },
  "required": ["products"]
}
